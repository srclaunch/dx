{
  "name": "@srclaunch/dx",
  "description": "Quality of life configuration and tooling for development environments.",
  "author": "Steven Bennett <steven@srclaunch.com>",
  "license": "MIT",
  "version": "0.1.71",
  "engines": {
    "node": ">=16",
    "yarn": ">=3.2.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "type": "module",
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    ".commitlintrc.mjs",
    ".commitlintrc.umd.cjs",
    ".eslintrc.mjs",
    ".eslintrc.umd.cjs",
    ".eslintrc.ui.mjs",
    ".eslintrc.ui.umd.cjs",
    ".prettierrc.mjs",
    ".prettierrc.umd.cjs",
    ".stylelintrc.mjs",
    ".stylelintrc.umd.cjs",
    ".stylelintrc.ui.mjs",
    ".stylelintrc.ui.umd.cjs",
    ".versionrc.mjs",
    ".versionrc.umd.cjs",
    "ava.config.mjs",
    "ava.config.umd.cjs",
    "jest.config.mjs",
    "jest.config.umd.cjs",
    "tsconfig.json",
    "tsconfig.ui.json"
  ],
  "module": "./dist/index.mjs",
  "exports": {
    "./.commitlintrc": {
      "import": "./.commitlintrc.mjs",
      "require": "./.commitlintrc.umd.cjs"
    },
    "./.eslintrc": {
      "import": "./.eslintrc.mjs",
      "require": "./.eslintrc.umd.cjs"
    },
    "./.eslintrc.ui": {
      "import": "./.eslintrc.ui.mjs",
      "require": "./.eslintrc.ui.umd.cjs"
    },
    "./.prettierrc": {
      "import": "./.prettierrc.mjs",
      "require": "./.prettierrc.umd.cjs"
    },
    "./.stylelintrc": {
      "import": "./.stylelintrc.mjs",
      "require": "./.stylelintrc.umd.cjs"
    },
    "./.stylelintrc.ui": {
      "import": "./.stylelintrc.ui.mjs",
      "require": "./.stylelintrc.ui.umd.cjs"
    },
    "./.versionrc": {
      "import": "./.versionrc.mjs",
      "require": "./.versionrc.umd.cjs"
    },
    "./ava.config": {
      "import": "./ava.config.mjs",
      "require": "./ava.config.umd.cjs"
    },
    "./jest.config": {
      "import": "./jest.config.mjs",
      "require": "./jest.config.umd.cjs"
    },
    "./tsconfig.json": {
      "import": "./tsconfig.json",
      "require": "./tsconfig.json"
    },
    "./tsconfig.ui.json": {
      "import": "./tsconfig.ui.json",
      "require": "./tsconfig.ui.json"
    }
  },
  "scripts": {
    "yui": "yarn upgrade-interactive",
    "qr": "yarn srclaunch changesets add --files \".\" --message \"Quick release\" --type \"chore\" && yarn srclaunch release --push",
    "build": "yarn srclaunch build",
    "release": "yarn srclaunch release"
  },
  "devDependencies": {
    "@types/node": "17.0.32",
    "standard-version": "9.3.2",
    "@srclaunch/cli": "0.1.516",
    "@srclaunch/types": "0.1.183"
  }
}